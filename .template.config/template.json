{
  "$schema": "http://json.schemastore.org/template",
  "author": "Felipe Azevedo",
"classifications": ["Solution", "Template", "Components", "Micro-Service", "Aurora-Tecnologia", "Fosforus"],
  "identity": "FosforusTemplateComponent",
  "name": "Fosforus Template Componet",
  "shortName": "FosforusSolution",
  "sourceName": "FosforusComponentTemplate",
  "preferNameDirectory": true,
  "symbols": {
    "SolutionName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "MySolution",
      "replaces": "MySolution"
    },
    "ProjectName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "MyComponent",
      "replaces": "MyComponent"
    }
  },
  "description": "This template is for solutions developed by Aurora Sistemas, ideal for components of the Fosforus framework.",
  "icons": {
    "default": "assets/icon.png"
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [ "**/[Bb]in/**", "**/[Oo]bj/**" ],
          "include": [ "README.md" ]
        }
      ]
    }
  ]
}
